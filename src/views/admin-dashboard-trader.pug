  .container-fluid.content-inner.pb-0
    div
      .row
        .col-sm-12
          .card
            .card-header
              h4.card-title.text-white Traders
            .d-flex.mt-3.ms-4.me-4.justify-content-between
            .card-body
              .table-responsive
                table.table
                  thead
                    tr
                      th Avatar
                      th Name
                      th Win Rate
                      th Profit Share
                      th 
                  tbody
                    td 
                        .mb-3
                          input.form-control#customFile(type='file' name="avatar" accept="image/*" required)
                    td
                        .form-floating.custom-form-floating.custom-form-floating-sm.form-group.mb-3
                            input.form-control#new-trader-name(type='text', placeholder='Name')
                    td
                        .form-floating.custom-form-floating.custom-form-floating-sm.form-group.mb-3
                            input.form-control#new-trader-winrate(type='number', placeholder='Win rate')
                    td
                        .form-floating.custom-form-floating.custom-form-floating-sm.form-group.mb-3
                            input.form-control#new-trader-profitshare(type='number', placeholder='Profit Share')
                    td
                        button.btn.btn-outline-success.update-btn#admin-new-trader(type='button') Add New Trader
                    each trader, index in traders
                        tr
                        td 
                          div
                            img.img-fluid.avatar.avatar-30px.avatar-rounded(src=`${trader.avatar}`, alt='trader avatar', style='width: 70px; height: 70px; object-fit: cover;')
                        td= `${trader.name}`
                        td= `${trader.winRate}%`
                        td= `${trader.profitShare}%`
                        //- td
                        //-     .form-floating.custom-form-floating.custom-form-floating-sm.form-group.mb-3
                        //-         input.form-control(type='number',placeholder='Place Holder', id=`balance-${index}`, data-existing-balance=user.balance)
