main.main-content
  .position-relative
    // Nav Start
    nav.nav.navbar.navbar-expand-lg.navbar-light.iq-navbar.border-bottom
      .container-fluid.navbar-inner
        a.navbar-brand(href='index.html').
          
          
        .sidebar-toggle(data-toggle='sidebar', data-active='true')
          i.icon
            svg(width='20px', height='20px', viewBox='0 0 24 24')
              path(fill='currentColor', d='M4,11V13H16L10.5,18.5L11.92,19.92L19.84,12L11.92,4.08L10.5,5.5L16,11H4Z')
        h4.title.
          
          Admin
          
        button.navbar-toggler(type='button', data-bs-toggle='collapse', data-bs-target='#navbarSupportedContent', aria-controls='navbarSupportedContent', aria-expanded='false', aria-label='Toggle navigation')
          span.navbar-toggler-icon
            span.navbar-toggler-bar.bar1.mt-2
            span.navbar-toggler-bar.bar2
            span.navbar-toggler-bar.bar3
        .collapse.navbar-collapse#navbarSupportedContent
          ul.navbar-nav.ms-auto.navbar-list.mb-2.mb-lg-0.align-items-center
            li.nav-item.dropdown
              a.nav-link.py-0.d-flex.align-items-center#navbarDropdown(href='#', role='button', data-bs-toggle='dropdown', aria-expanded='false')
                img.img-fluid.avatar.avatar-50.avatar-rounded(src='/assets/images/avatars/01.png', alt='User-Profile')
                .caption.ms-3
                  h6.mb-0.caption-title= `${user.firstName} ${user.lastName}`
                  p.mb-0.caption-sub-title= `${user.role}`
              ul.dropdown-menu.dropdown-menu-end(aria-labelledby='navbarDropdown')
                li.border-0
                  a#logoutLink.dropdown-item(href='') Logout
                  
    // Nav End
  .container-fluid.content-inner.pb-0
    div
      .row
        .col-sm-12
          .card
            .card-header
              h4.card-title.text-white Approved Transactions
            .d-flex.mt-3.ms-4.me-4.justify-content-between
            .card-body
              .table-responsive
                table.table
                  if transactions.length > 0
                    thead
                      tr
                        th First Name
                        th Last Name
                        th Transaction Type
                        th Amount
                        th Date
                        th Status
                    tbody
                      each transaction in transactions
                          tr
                          td= transaction.user.firstName
                          td= transaction.user.lastName
                          td= transaction.type
                          td= `$${transaction.amount}`
                          td= transaction.date
                          td Approved
                  else 
                    p No approved transactions found


